#include<iostream>
using namespace std;
void reversearray(int arr[],int size){
    int start=0;
    int end=0;
    while(start<end){
        swap(arr[start],arr[end]);
        start++;
        end--;
    
    }
}
int main(){
    int arr[]={2,3,4,5};
    int size=4;
    reversearray(arr,size);
    for(int i=0;i<size;i++){
        cout<<arr[i]<<" ";

    }
    cout<<endl;
    return 0;

}

