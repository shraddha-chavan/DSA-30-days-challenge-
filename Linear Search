#include<iostream>
using namespace std;
    int linearsearch(int arr,int size,int target){
        for(int i=0;i<size;i++){
            if (arr[i]==target){
                return i;
            }
        }
        return -1;
    int main(){
        int arr[]={2,4,0,1,9};
        int size=5;
        int target=1;
        cout<<"index "<<arr,size,target<<endl;
        return 0;
    }

